   "source": [
    "7. Определить, является ли год, который ввел пользователь, високосным или не високосным."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 31,
   "id": "39afd250",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Введите год: 2022\n",
      "Не високосный год\n"
     ]
    }
   ],
   "source": [
    "year = int(input('Введите год: '))\n",
    " \n",
    "if year % 4 != 0 or (year % 100 == 0 and year % 400 != 0):\n",
    "    print(\"Не високосный год\")\n",
    "else:\n",
    "    print(\"Високосный год\")"
   ]
  }
